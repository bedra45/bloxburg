Tab1:AddButton({
	Name = "FullBright",
	Callback = function()
local Lighting = game.Lighting

game.RunService.RenderStepped:Connect(function()
		Lighting.Brightness = 2
		Lighting.ClockTime = 14
		Lighting.FogEnd = 100000
		Lighting.GlobalShadows = false
		Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
end)
end  
})

local Section = Tab1:AddSection({
	Name = "Gamepasses"
})

Tab1:AddButton({
	Name = "Get Radio Gamepass",
	Callback = function()
local de = game.Players.LocalPlayer

for i,v in pairs(game.ReplicatedStorage.Stats[de.Name].Gamepasses:GetChildren()) do
    if v.Name == "1950751" then
        v.Value = true
    end
end
end  
})

local Tab4 = Window:MakeTab({
	Name = "Tp Stuff",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab4:AddButton({
	Name = "Tp to my plot",
	Callback = function()
remotes["ToPlot"]:InvokeServer({})
end  
})


Tab4:AddTextbox({
	Name = "Tp to any plot",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		remotes["ToPlot"]:InvokeServer({["Player"] = game:GetService("Players")[Value]})
	end	  
})

local Tab3 = Window:MakeTab({
	Name = "Vehicle Stats",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab3:AddSection({
	Name = "IMPORTANT: REDRIVE THE VEHICLE ONCE U CHANGED A VALUE"
})

Tab3:AddSlider({
	Name = "ForwardSpeed",
	Min = 0,
	Max = 100,
	Default = 68,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Value",
	Callback = function(Uhm)
		
local VehicleData = require(game.ReplicatedStorage.Modules.VehicleService.VehicleData);

		for i,v in next, VehicleData do 
			v.ForwardSpeed = Uhm
		end
		
	end    
})

Tab3:AddSlider({
	Name = "ReverseSpeed",
	Min = 0,
	Max = 100,
	Default = 30,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Value",
	Callback = function(Uhm)
		
local VehicleData = require(game.ReplicatedStorage.Modules.VehicleService.VehicleData);

		for i,v in next, VehicleData do 
			v.ReverseSpeed = Uhm
		end
		
	end    
})

